# ─── Dev server ────────────────────────────────────────────────────────────────
VITE_DEV_PORT=5173
VITE_API_TARGET=http://127.0.0.1:3000

# ─── Ads / Prebid / GAM ───────────────────────────────────────────────────────
# prebid | google | (порожнє, щоб вимкнути)
VITE_ADS_MODULE=prebid

VITE_ENABLE_PREBID=true
VITE_ENABLE_GAM=false
VITE_ADS_DEBUG=true

VITE_ENABLE_BIDMATIC=false
VITE_BIDMATIC_SOURCE=0
VITE_BIDMATIC_SPN=

VITE_GAM_NETWORK=false
VITE_ENABLE_REPORTING=true

# Якщо ваш adserver клієнт читає це (public/modules/adserver.client.js)
VITE_ADSERVER_ENDPOINT=

# ─── API / інші URL (якщо потрібно у вашій логіці) ────────────────────────────
VITE_API_URL=
VITE_ADSERVER_URL=

# ─── Sentry ───────────────────────────────────────────────────────────────────
# DSN для рантайм-ініціалізації у браузері
VITE_SENTRY_DSN=

# Завантаження сорс-мапів на релізі (CI)
SENTRY_UPLOAD=false
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_AUTH_TOKEN=
# Зазвичай підставляєте з CI, напр. git SHA
SENTRY_RELEASE=
GITHUB_SHA=