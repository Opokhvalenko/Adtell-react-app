{
	"buildCommand": "npm run build",
	"outputDirectory": "dist",
	"framework": "vite",
	"rewrites": [
		{
			"source": "/api/:path*",
			"destination": "https://addtell-backend.onrender.com/api/:path*"
		},
		{
			"source": "/ads/:path*",
			"destination": "https://addtell-backend.onrender.com/ads/:path*"
		},
		{
			"source": "/create-lineitem",
			"destination": "https://addtell-backend.onrender.com/create-lineitem"
		},
		{ "source": "/(.*)", "destination": "/" }
	]
}
