{
	"name": "cladtell-react-app",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"tailwind:init": "tailwindcss init -y",
		"tw:dev": "tailwindcss -c tailwind.config.js -i ./src/index.css -o ./src/tailwind.css --watch",
		"tw:build": "tailwindcss -c tailwind.config.js -i ./src/index.css -o ./src/tailwind.css --minify",
		"tw:embed": "tailwindcss -c tailwind.config.js -i ./src/ssr/embed.css -o ./public/assets/tw-embed.css --minify",
		"prebid:copy": "node -e \"(function(){const fs=require('fs');const p=require('path');const out='public/prebid/prebid.js';fs.mkdirSync(p.dirname(out),{recursive:true});function exists(f){try{return fs.statSync(f).isFile()}catch(e){return false}}let src=null;try{src=require.resolve('prebid.js/dist/prebid.js')}catch(e){}if(!src){try{src=require.resolve('prebid.js/build/dist/prebid.js')}catch(e){}}if(!src&&exists('public/prebid/prebid10.10.0.js')){src='public/prebid/prebid10.10.0.js'}if(src){fs.copyFileSync(src,out);console.log('Prebid copied from',src)}else{console.log('No local Prebid bundle found; CDN fallback will be used')}})();\"",
		"prebid": "npm run prebid:copy",
		"dev": "vite",
		"build": "npm run prebid && npm run tw:build && vite build",
		"clean": "rimraf dist",
		"rebuild": "npm run clean && npm run build",
		"preview": "vite preview",
		"lint": "biome check --diagnostic-level=error .",
		"biome:check": "biome check --diagnostic-level=error .",
		"biome:fix": "biome check --write .",
		"biome:fix:unsafe": "biome check --write --unsafe .",
		"format": "biome format --write .",
		"type-check": "tsc --noEmit -p tsconfig.json",
		"type-check:watch": "tsc -w --noEmit -p tsconfig.json",
		"check": "npm run type-check && npm run biome:check",
		"test": "vitest --passWithNoTests",
		"test:run": "vitest run --passWithNoTests",
		"test:ui": "vitest --ui",
		"prepare": "husky"
	},
	"engines": {
		"node": ">=18.18 <23"
	},
	"dependencies": {
		"@headlessui/react": "^2.2.8",
		"@hookform/resolvers": "^5.2.2",
		"@sentry/react": "^10.17.0",
		"@sentry/replay": "^7.116.0",
		"@sentry/tracing": "^7.120.4",
		"@tanstack/react-query": "^5.89.0",
		"@tanstack/react-table": "^8.21.3",
		"p-retry": "^7.0.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-hook-form": "^7.62.0",
		"react-router-dom": "^7.9.1",
		"recharts": "^3.2.1",
		"tailwind-merge": "^3.3.1",
		"use-sync-external-store": "^1.5.0",
		"zod": "^4.1.8",
		"zustand": "^5.0.8"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@sentry/vite-plugin": "^4.3.0",
		"@tailwindcss/cli": "^4.1.13",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.14",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^24.5.1",
		"@types/react": "^19.2.0",
		"@types/react-dom": "^19.2.0",
		"@vitejs/plugin-react": "^5.0.0",
		"@vitest/coverage-v8": "^3.2.4",
		"autoprefixer": "^10.4.21",
		"husky": "^9.1.7",
		"jsdom": "^27.0.0",
		"postcss": "^8.5.6",
		"prebid.js": "^10.12.0",
		"rollup-plugin-visualizer": "^6.0.3",
		"tailwindcss": "^4.1.14",
		"terser": "^5.44.0",
		"typescript": "~5.8.3",
		"vite": "^7.1.2",
		"vite-plugin-checker": "^0.10.3",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-inspect": "^11.3.3",
		"vite-plugin-svgr": "^4.5.0",
		"vitest": "^3.2.4"
	}
}
