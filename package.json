{
	"name": "cladtell-react-app",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"tailwind:init": "tailwindcss init -y",
		"tw:dev": "tailwindcss -c tailwind.config.js -i ./src/index.css -o ./src/tailwind.css --watch",
		"tw:build": "tailwindcss -c tailwind.config.js -i ./src/index.css -o ./src/tailwind.css --minify",
		"dev": "vite",
		"build": "npm run tw:build && vite build",
		"preview": "vite preview",
		"lint": "biome check --diagnostic-level=error .",
		"biome:check": "biome check --diagnostic-level=error .",
		"biome:fix": "biome check --write .",
		"biome:fix:unsafe": "biome check --write --unsafe .",
		"format": "biome format --write .",
		"type-check": "tsc --noEmit -p tsconfig.json",
		"check": "npm run type-check && npm run biome:check",
		"test": "vitest --passWithNoTests",
		"test:run": "vitest run --passWithNoTests",
		"test:ui": "vitest --ui"
	},
	"engines": {
		"node": ">=18.18 <23"
	},
	"dependencies": {
		"@hookform/resolvers": "^5.2.2",
		"@radix-ui/react-dialog": "^1.1.15",
		"@tanstack/react-query": "^5.89.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-hook-form": "^7.62.0",
		"react-router-dom": "^7.9.1",
		"zod": "^4.1.8",
		"zustand": "^5.0.8"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@playwright/test": "^1.55.0",
		"@tailwindcss/cli": "^4.1.13",
		"@tailwindcss/forms": "^0.5.10",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/react": "^16.3.0",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.5.1",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"@vitejs/plugin-react": "^5.0.0",
		"depcheck": "^1.4.7",
		"husky": "^9.1.7",
		"jsdom": "^27.0.0",
		"madge": "^8.0.0",
		"rollup-plugin-visualizer": "^6.0.3",
		"tailwindcss": "^4.1.13",
		"terser": "^5.44.0",
		"ts-prune": "^0.10.3",
		"typescript": "~5.8.3",
		"vite": "^7.1.2",
		"vite-plugin-checker": "^0.10.3",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-inspect": "^11.3.3",
		"vite-plugin-svgr": "^4.5.0",
		"vitest": "^3.2.4"
	}
}
